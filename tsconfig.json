{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/config/categories.ts.bak", "app/air-fryers/page.tsx.bak", "app/blenders/page.tsx.bak", "app/cameras/page.tsx.bak", "app/doorbells/page.tsx.bak", "app/external-hard-drives/page.tsx.bak", "app/food-storage/page.tsx.bak", "app/headphones/page.tsx.bak", "app/monitors/page.tsx.bak", "app/slow-cookers/page.tsx.bak", "app/speakers/page.tsx.bak", "app/tumblers/page.tsx.bak"],
  "exclude": ["node_modules"]
}
